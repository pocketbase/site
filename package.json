{
  "name": "pocketbase-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "jstypes": "npx typedoc --options ./jsvm/typedoc.json",
    "dev": "vite dev",
    "build": "vite build && rm -rf static/pagefind && npx pagefind --site build --output-path static/pagefind --glob docs/**/*.{html}",
    "preview": "vite preview"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 110,
    "svelteBracketNewLine": true
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "luxon": "^3.5.0",
    "prismjs": "^1.29.0",
    "sass": "^1.56.2",
    "svelte": "^4.0.0",
    "typedoc": "^0.24.8",
    "typedoc-theme-hierarchy": "^4.0.0",
    "vite": "^5.4.10",
    "pagefind": "^1.1.1"
  }
}
